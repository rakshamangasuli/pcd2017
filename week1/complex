#include<stdio.h>
 
     struct complex
{
     float real;
     float imaginary;
};

 struct complex getcomplex()
{
     struct complex c;
     printf("Enter real and imaginary parts of a complex no.\n");
     scanf("%f%f,&c.real ,&c.imaginary");
     return c;
}

int main()
{
      complex a;
      complex b;
      complex c;
      a.getcomplex();
      b.getcomplex();
      c=add(a,b);
      printcomplex(c);
      return 0;
}

struct complex add(complex x, complex y)
{
    complex c;
    c.real=a.real+b.real;
    c.complex=a.imaginary+b.imaginary;
    return c;
}

void printcomplex()
{
    printf("%f%f, &c.real,&c.imaginary");
}

